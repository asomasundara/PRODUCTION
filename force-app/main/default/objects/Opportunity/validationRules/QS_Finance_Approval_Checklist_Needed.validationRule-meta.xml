<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>QS_Finance_Approval_Checklist_Needed</fullName>
    <active>true</active>
    <description>,</description>
    <errorConditionFormula>AND ( ISPICKVAL (Finance_Status__c, &quot;Approved&quot;), Finance_Checklist_Complete__c = FALSE)</errorConditionFormula>
    <errorDisplayField>Finance_Status__c</errorDisplayField>
    <errorMessage>You cannot approve an opportunity unless the finance checklist is complete.</errorMessage>
</ValidationRule>
