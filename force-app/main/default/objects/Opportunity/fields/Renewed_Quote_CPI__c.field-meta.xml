<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Renewed_Quote_CPI__c</fullName>
    <description>On a Contract Renewal, this field stores the value of CPI entered on the renewed quote. This field is referenced in a price rule</description>
    <formula>IF( SBQQ__RenewedContract__r.SBQQ__Quote__r.IncludeCPIinQuote__c = TRUE, SBQQ__RenewedContract__r.SBQQ__Quote__r.CPI__c,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Renewed Quote CPI</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Percent</type>
</CustomField>
