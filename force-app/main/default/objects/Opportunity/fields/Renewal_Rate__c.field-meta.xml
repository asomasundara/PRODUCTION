<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Renewal_Rate__c</fullName>
    <description>Renewal Rate = ARR - (Price Increase + Product Bookings)</description>
    <formula>AnnualRecurringRevenue__c - ( Total_Price_Increase__c + Total_Product_Bookings_Amount__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Renewal Rate less Price Increases &amp; Product Bookings</inlineHelpText>
    <label>Renewal Rate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
